{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
      "src/*"
      ],
      "@icons/*": [
      "assets/Icons/*"
      ],
      "@components/*": [
      "src/components/*"
      ],
      "@pages/*": [
      "src/pages/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
